function change2(n):void{
	if(chk_r[n]){
		chk_r[n] = false;
		checks2[n].gotoAndStop(1);
		chekr--;
	} else{
		if(chekr < 3){
			chk_r[n] = true;
			checks2[n].gotoAndStop(2);
			chekr++;
		}
	}
}

check1_r.addEventListener(MouseEvent.CLICK, check1r);
function check1r(e:MouseEvent){
	var n = 0;
	change2(n);
}

check2_r.addEventListener(MouseEvent.CLICK, check2r);
function check2r(e:MouseEvent){
	var n = 1;
	change2(n);
}

check3_r.addEventListener(MouseEvent.CLICK, check3r);
function check3r(e:MouseEvent){
	var n = 2;
	change2(n);
}

check4_r.addEventListener(MouseEvent.CLICK, check4r);
function check4r(e:MouseEvent){
	var n = 3;
	change2(n);
}

check5_r.addEventListener(MouseEvent.CLICK, check5r);
function check5r(e:MouseEvent){
