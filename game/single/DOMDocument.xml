/* Check Blue */

var chekb = 0;
var chk_b:Array = new Array(false, false, false, false, false, false);
var checks1:Array = new Array(check1_b, check2_b, check3_b, check4_b, check5_b, check6_b);

function change1(n):void{
	if(chk_b[n]){
		chk_b[n] = false;
		checks1[n].gotoAndStop(1);
		chekb--;
	} else{
		if(chekb < 3){
			chk_b[n] = true;
			checks1[n].gotoAndStop(2);
			chekb++;
		}
	}
}

check1_b.addEventListener(MouseEvent.CLICK, check1b);
function check1b(e:MouseEvent){
	var n = 0;
	change1(n);
}

check2_b.addEventListener(MouseEvent.CLICK, check2b);
function check2b(e:MouseEvent){
	var n = 1;
	change1(n);
}

